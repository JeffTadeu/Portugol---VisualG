Algoritmo "consumo"


Var

   distancia, combust, CM : real

Inicio

   escreval("Por favor, escreva a distância percorrida: ")
   leia(distancia)
   escreval("Agora digite o combustível gasto: ")
   leia(combust)
   CM <- distancia / combust
   escreval("O valor do consumo médio é de ", CM:2:3)

Fimalgoritmo